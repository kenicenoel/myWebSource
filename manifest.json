{
  "manifest_version": 2,
  "name": "my Websource",
  "description": "Fast access to your Web Source account",
  "version": "1.0.1",
  "author": "Kenice Noel",
  "incognito": "spanning",

  "browser_action":
  {
    "default_icon":"images/icon64.png",
    "default_popup": "start.html"

  },


  "icons":
  {
    "16": "images/icon16.png",
    "48": "images/icon48.png"

  },

  "permissions":
              [
                "storage",
                "http://localhost/packagehero*"
              ]

}
